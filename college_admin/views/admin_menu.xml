    name="College Students"
    res_model="college.student"
    view_mode="tree,form"
  />

  <menuitem id="menu_admin_department"
            name="Departments"
      action="action_admin_department"
            parent="college_admin_menu" />
  <menuitem id="menu_admin_staff_admin"
            name="Admin Staff"
