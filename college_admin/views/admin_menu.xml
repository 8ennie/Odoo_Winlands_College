            name="College Admin" />

  <act_window id="action_admin_department"
    name="College Departments"
    res_model="college.department"
    view_mode="tree,form"
  />
  <act_window id="action_admin_staff_admin"
    name="College Admin Staff"
    res_model="college.staff.admin"
    view_mode="tree,form"
  />
  <act_window id="action_admin_staff_academic"
    name="College Academic Staff"
    res_model="college.staff.academic"
    view_mode="tree,form"
  />
