<?xml version="1.0"?>
<odoo>

  <record id="view_form_program" model="ir.ui.view">
    <field name="name">Program Form</field>
    <field name="model">college.program</field>
    <field name="arch" type="xml">
      <form string="Program">
    <sheet>
          <group>
            <field name="Program Name" />
            <field name="Program Length" />
            <field name="Program Qualification" />
          </group>
    </sheet>
      </form>
    </field>
  </record>

  <record id="view_tree_program" model="ir.ui.view"> 
    <field name="name">Program List</field>
    <field name="model">college.program</field>
    <field name="arch" type="xml">
      <tree>
        <field name="Program Name" />
        <field name="Program Length" />
        <field name="Program Qualification" />
      </tree>
    </field>
  </record>
</odoo>
