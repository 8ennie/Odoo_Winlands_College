<?xml version="1.0"?>
<odoo>

  <record id="view_form_staff" model="ir.ui.view">
    <field name="name">Staff Form</field>
    <field name="model">college.staff</field>
    <field name="arch" type="xml">
      <form string="Staff">
    <sheet>
          <group>
            <field name="partner_id" />
            <field name="staff_email" />
          </group>
    </sheet>
      </form>
    </field>
  </record>

  <record id="view_tree_staff" model="ir.ui.view">
    <field name="name">Staff List</field>
    <field name="model">college.staff</field>
    <field name="arch" type="xml">
      <tree>
        <field name="partner_id" />
        <field name="staff_email" />
      </tree>
    </field>
  </record>
</odoo>
