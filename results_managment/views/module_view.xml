<?xml version="1.0"?>
<odoo>

  <record id="view_form_module" model="ir.ui.view">
    <field name="name">Module Form</field>
    <field name="model">college.module</field>
    <field name="arch" type="xml">
      <form string="Module">
    <sheet>
          <group>
            <field name="module_students" />
            <field name="module_name" />
            <field name="module_time_frame" />
            <field name="module_credits" />
          </group>
    </sheet>
      </form>
    </field>
  </record>

  <record id="view_tree_module" model="ir.ui.view">
    <field name="name">Module List</field>
    <field name="model">college.module</field>
    <field name="arch" type="xml">
      <tree>
        <field name="module_students" />
        <field name="module_name" />
        <field name="module_time_frame" />
        <field name="module_credits" />
      </tree>
    </field>
  </record>
</odoo>
