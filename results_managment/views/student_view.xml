<?xml version="1.0"?>
<odoo>

  <record id="view_form_student" model="ir.ui.view">
    <field name="name">Student Form</field>
    <field name="model">college.student</field>
    <field name="arch" type="xml">
      <form string="Student">
    <sheet>
          <group>
            <field name="Student Number" />
            <field name="Email" />
            <field name="Module" />
            <field name="Program" />
            <field name="Starting Year" />
          </group>
    </sheet>
      </form>
    </field>
  </record>

  <record id="view_tree_student" model="ir.ui.view">
    <field name="name">Student List</field>
    <field name="model">college.student</field>
    <field name="arch" type="xml">
      <tree>
        <field name="Student Number" />
        <field name="Email" />
        <field name="Module" />
        <field name="Program" />
        <field name="Starting Year" />
      </tree>
    </field>
  </record>
</odoo>
